<filter-conf>
    <version>20210524</version>
    <!--安规总开关-->
    <high_temperature_switch>true</high_temperature_switch>
    <!-- 35度安规关闭闪光灯温度阈值 -->
    <high_temperature_close_flash_lights>460</high_temperature_close_flash_lights>
    <!-- 35度安规关闭热点温度阈值 -->
    <high_temperature_close_hotspot>470</high_temperature_close_hotspot>
    <!-- 35度安规退应用温度阈值 -->
    <high_temperature_close_apps>600</high_temperature_close_apps>
    <!-- 35度安规紧急拨号温度阈值 -->
    <high_temperature_only_emergencycall>490</high_temperature_only_emergencycall>

    <!-- 25度安规关闭闪光灯温度阈值 -->
    <high_temperature_close_flash_lights_25_degree>400</high_temperature_close_flash_lights_25_degree>
    <!-- 25度安规关闭热点温度阈值 -->
    <high_temperature_close_hotspot_25_degree>400</high_temperature_close_hotspot_25_degree>
    <!-- 25度安规恢复热点温度阈值 -->
    <high_temperature_restore_hotspot_25_degree>390</high_temperature_restore_hotspot_25_degree>
    <!-- 25度安规退应用温度阈值 -->
    <high_temperature_close_apps_25_degree>400</high_temperature_close_apps_25_degree>
    <!-- 25度安规国家-->
    <high_temperature_25_degree_country>VN ID MM PK BD NP LK KE PH MY KH TH TR EG UA KZ SA MA IL RU</high_temperature_25_degree_country>
    <!-- 35度安规国家-->
    <high_temperature_35_degree_country>MX BR LATAM IN</high_temperature_35_degree_country>
    <!--25度安规转换35度安规阈值-->
    <high_temperature_transition_threshold>300</high_temperature_transition_threshold>
    <!--25度安规转换35度安规灭屏间隔时间阈值，单位：毫秒-->
    <high_temperature_screen_off_interval_time>600000</high_temperature_screen_off_interval_time>
    <!--25度安规场景识别集合，分别代表：充电、闪光灯\补光灯、热点、Camera、后台导航\音乐，1/0 代表 开启/关闭-->
    <safety_test_config_item>01111,10111,11011,11101,11110,00111,01011,01101,01110,10011,10101,10110,11001,11010,11100,11000,11111</safety_test_config_item>
</filter-conf>
